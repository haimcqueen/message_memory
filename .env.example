# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_key_here

# Whapi Configuration
WHAPI_TOKEN=your_whapi_bearer_token_here

# OpenAI Configuration
OPENAI_API_KEY=sk-your_openai_api_key_here

# OpenAI Models (optional - defaults provided)
OPENAI_PDF_MODEL=gpt-5.1-2025-11-13
OPENAI_SESSION_MODEL=gpt-4o-mini
OPENAI_TRANSCRIPTION_MODEL=whisper-1

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Environment
ENVIRONMENT=development

# n8n Webhook Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.app.n8n.cloud/webhook/your-webhook-id
N8N_WEBHOOK_API_KEY=your_n8n_api_key_here
N8N_BATCH_DELAY_SECONDS=60
